{
  "name": "avalon2-mmRouter-starter",
  "version": "0.17.1",
  "description": "a simple starter with avalon2 and mmRouter",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node build/server.js",
    "dll": "webpack -p --optimize-minimize --config build/webpack.dll.config.js",
    "build": "rimraf ./dist && cross-env NODE_ENV=production node build/prod.js",
    "preview": "http-server ./dist -p 1234 -c-1",
    "redll": "rimraf build/dist && npm run dll && npm start",
    "deploy": "rimraf build/dist && npm run dll && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbbfcd/avalon2-mmRouter-starter.git"
  },
  "keywords": [
    "avalon2",
    "mmRouter"
  ],
  "author": "cbbfcd",
  "license": "MIT",
  "dependencies": {
    "avalon2": "latest",
    "lodash": "^4.17.4",
    "mmRouter": "latest"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "css-declaration-sorter": "^4.0.1",
    "css-mqpacker": "^7.0.0",
    "es6-promise": "^4.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "express": "^4.13.3",
    "express-favicon": "^2.0.1",
    "faker": "^4.1.0",
    "file-loader": "^4.2.0",
    "glob": "7.1.4",
    "happypack": "^5.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "image-webpack-loader": "^6.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "nyan-progress-webpack-plugin": "^1.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-cssnext": "^3.1.0",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-dev-server": "^3.8.2",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-merge": "latest"
  },
  "bugs": {
    "url": "https://github.com/cbbfcd/avalon2-mmRouter-starter/issues"
  },
  "homepage": "https://github.com/cbbfcd/avalon2-mmRouter-starter#readme"
